et shipping to true since under 25.00\n            hasShipping = true;\n          return acc + parseFloat(value.salePrice) + 5.99;\n        } else {\n          return acc + parseFloat(value.salePrice);\n        }\n      }, 0)\n      .toFixed(2);\n      // get cart total & shipping value\n      return { cartTotal, hasShipping };\n  };\n\n  const { cartTotal, hasShipping } = getCartTotal();\n\n  return (\n    <CartContainer>\n      {value.cart.map((item) => (\n        <Item key={item.id} {...item} />\n      ))}\n      <p className={hasShipping === true ? \"show-shipping\" : \"hide-shipping\"}>Shipping: $5.99</p>\n\n      <div>\n        <p>Total: ${cartTotal}</p>\n        <button>Checkout</button>\n      </div>\n    </CartContainer>\n  );\n};\n\nexport default Cart;\n","/Users/erica/Desktop/honey-pot/my-app/src/components/LandingPage.js",[],"/Users/erica/Desktop/honey-pot/my-app/src/contexts/CartContext.js",[],"/Users/erica/Desktop/honey-pot/my-app/src/data/dummyData.js",[],"/Users/erica/Desktop/honey-pot/my-app/src/contexts/AccessoriesContext.js",[],"/Users/erica/Desktop/honey-pot/my-app/src/contexts/BeautyContext.js",[],"/Users/erica/Desktop/honey-pot/my-app/src/contexts/CollectiblesContext.js",[],"/Users/erica/Desktop/honey-pot/my-app/src/contexts/HomeGoodsContext.js",[],"/Users/erica/Desktop/honey-pot/my-app/src/components/ProductDetails.js",[],"/Users/erica/Desktop/honey-pot/my-app/src/components/ItemInCart.js",[],{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","severity":1,"message":"80","line":1,"column":29,"nodeType":"81","messageId":"82","endLine":1,"endColumn":37},"no-native-reassign",["83"],"no-negated-in-lhs",["84"],"no-unused-vars","'useState' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]